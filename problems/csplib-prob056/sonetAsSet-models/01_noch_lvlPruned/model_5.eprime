language ESSENCE' 1.0

given nnodes: int(1..)
given nrings: int(1..)
given capacity: int(1..)
given fin1: int
given demand_ExplicitR3_Explicit: matrix indexed by [int(1..fin1), int(1..2)] of int(1..nnodes)
find network_ExplicitVarSizeWithFlagsR2_Flags: matrix indexed by [int(1..nrings)] of bool
find network_ExplicitVarSizeWithFlagsR2_Values_Occurrence: matrix indexed by [int(1..nrings), int(1..nnodes)] of bool
letting let1 be nrings * capacity
find optVar: int(0..let1)
branching on [network_ExplicitVarSizeWithFlagsR2_Flags, network_ExplicitVarSizeWithFlagsR2_Values_Occurrence, optVar]
minimising optVar
such that
    optVar =
    sum([toInt(network_ExplicitVarSizeWithFlagsR2_Flags[q10]) *
         catchUndef(sum([toInt(network_ExplicitVarSizeWithFlagsR2_Values_Occurrence[q10, q11]) | q11 : int(1..nnodes)]),
                    0)
             | q10 : int(1..nrings)]),
    and([or([network_ExplicitVarSizeWithFlagsR2_Flags[q13] /\
             and([network_ExplicitVarSizeWithFlagsR2_Values_Occurrence[q13, demand_ExplicitR3_Explicit[q12, q15]]
                      | q15 : int(1..2)])
                 | q13 : int(1..nrings)])
             | q12 : int(1..fin1)]),
    and([network_ExplicitVarSizeWithFlagsR2_Flags[q1 + 1] ->
         [-toInt(network_ExplicitVarSizeWithFlagsR2_Values_Occurrence[q1, q7]) | q7 : int(1..nnodes)] <lex
         [-toInt(network_ExplicitVarSizeWithFlagsR2_Values_Occurrence[q1 + 1, q8]) | q8 : int(1..nnodes)]
             | q1 : int(1..nrings - 1)]),
    and([network_ExplicitVarSizeWithFlagsR2_Flags[q2] = false ->
         and([network_ExplicitVarSizeWithFlagsR2_Values_Occurrence[q2, q9] = false | q9 : int(1..nnodes)])
             | q2 : int(1..nrings)]),
    and([network_ExplicitVarSizeWithFlagsR2_Flags[q3 + 1] -> network_ExplicitVarSizeWithFlagsR2_Flags[q3]
             | q3 : int(1..nrings - 1)]),
    sum([toInt(network_ExplicitVarSizeWithFlagsR2_Flags[q4]) | q4 : int(1..nrings)]) <= nrings,
    and([network_ExplicitVarSizeWithFlagsR2_Flags[q5] ->
         2 <= sum([toInt(network_ExplicitVarSizeWithFlagsR2_Values_Occurrence[q5, q6]) | q6 : int(1..nnodes)])
             | q5 : int(1..nrings)]),
    and([network_ExplicitVarSizeWithFlagsR2_Flags[q5] ->
         sum([toInt(network_ExplicitVarSizeWithFlagsR2_Values_Occurrence[q5, q6]) | q6 : int(1..nnodes)]) <= capacity
             | q5 : int(1..nrings)])

