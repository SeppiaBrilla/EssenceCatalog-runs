language ESSENCE' 1.0

given nnodes: int(1..)
given nrings: int(1..)
given capacity: int(1..)
given fin1: int
given demand_ExplicitR3_Explicit: matrix indexed by [int(1..fin1), int(1..2)] of int(1..nnodes)
find network_ExplicitVarSizeWithFlagsR6_Flags: matrix indexed by [int(1..nrings)] of bool
find network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy:
        matrix indexed by [int(1..nrings), int(1..capacity)] of int(1..nnodes + 1)
letting let1 be nrings * capacity
find optVar: int(0..let1)
branching on
    [network_ExplicitVarSizeWithFlagsR6_Flags, network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy,
     optVar]
minimising optVar
such that
    optVar =
    sum([toInt(network_ExplicitVarSizeWithFlagsR6_Flags[q13]) *
         catchUndef(sum([toInt(network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q13, q15] !=
                               nnodes + 1)
                             | q15 : int(1..capacity)]),
                    0)
             | q13 : int(1..nrings)]),
    and([or([network_ExplicitVarSizeWithFlagsR6_Flags[q17] /\
             and([or([network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q17, q21] != nnodes + 1 /\
                      network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q17, q21] =
                      demand_ExplicitR3_Explicit[q16, q19]
                          | q21 : int(1..capacity)])
                      | q19 : int(1..2)])
                 | q17 : int(1..nrings)])
             | q16 : int(1..fin1)]),
    and([network_ExplicitVarSizeWithFlagsR6_Flags[q1 + 1] ->
         [network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q1, q10] | q10 : int(1..capacity)] <lex
         [network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q1 + 1, q11] | q11 : int(1..capacity)]
             | q1 : int(1..nrings - 1)]),
    and([network_ExplicitVarSizeWithFlagsR6_Flags[q2] = false ->
         and([network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q2, q12] = 1 | q12 : int(1..capacity)])
             | q2 : int(1..nrings)]),
    and([network_ExplicitVarSizeWithFlagsR6_Flags[q3 + 1] -> network_ExplicitVarSizeWithFlagsR6_Flags[q3]
             | q3 : int(1..nrings - 1)]),
    sum([toInt(network_ExplicitVarSizeWithFlagsR6_Flags[q4]) | q4 : int(1..nrings)]) <= nrings,
    and([network_ExplicitVarSizeWithFlagsR6_Flags[q5] ->
         and([network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q5, q6] <
              network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q5, q6 + 1]
              \/ network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q5, q6] = nnodes + 1
                  | q6 : int(1..capacity - 1)])
             | q5 : int(1..nrings)]),
    and([network_ExplicitVarSizeWithFlagsR6_Flags[q5] ->
         and([network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q5, q7] = nnodes + 1 ->
              network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q5, q7 + 1] = nnodes + 1
                  | q7 : int(1..capacity - 1)])
             | q5 : int(1..nrings)]),
    and([network_ExplicitVarSizeWithFlagsR6_Flags[q5] ->
         2 <=
         sum([toInt(network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q5, q8] != nnodes + 1)
                  | q8 : int(1..capacity)])
             | q5 : int(1..nrings)]),
    and([network_ExplicitVarSizeWithFlagsR6_Flags[q5] ->
         sum([toInt(network_ExplicitVarSizeWithFlagsR6_Values_ExplicitVarSizeWithDummy[q5, q8] != nnodes + 1)
                  | q8 : int(1..capacity)])
         <= capacity
             | q5 : int(1..nrings)])

