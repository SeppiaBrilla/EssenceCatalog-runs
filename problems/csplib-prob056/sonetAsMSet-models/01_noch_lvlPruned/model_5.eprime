language ESSENCE' 1.0

given nnodes: int(1..)
given nrings: int(1..)
given capacity: int(1..)
given fin1: int
given demand_ExplicitR3_Explicit: matrix indexed by [int(1..fin1), int(1..2)] of int(1..nnodes)
find network_ExplicitWithRepetitionR2_Flag: int(nrings)
find network_ExplicitWithRepetitionR2_Values_Occurrence: matrix indexed by [int(1..nrings), int(1..nnodes)] of bool
letting let1 be nrings * capacity
find optVar: int(0..let1)
branching on [network_ExplicitWithRepetitionR2_Flag, network_ExplicitWithRepetitionR2_Values_Occurrence, optVar]
minimising optVar
such that
    optVar =
    sum([sum([toInt(network_ExplicitWithRepetitionR2_Values_Occurrence[q10, q11]) | q11 : int(1..nnodes)])
             | q10 : int(1..nrings), q10 <= nrings]),
    and([or([and([network_ExplicitWithRepetitionR2_Values_Occurrence[q13, demand_ExplicitR3_Explicit[q12, q15]]
                      | q15 : int(1..2)])
                 | q13 : int(1..nrings), q13 <= nrings])
             | q12 : int(1..fin1)]),
    and([[-toInt(network_ExplicitWithRepetitionR2_Values_Occurrence[q1, q7]) | q7 : int(1..nnodes)] <=lex
         [-toInt(network_ExplicitWithRepetitionR2_Values_Occurrence[q1 + 1, q8]) | q8 : int(1..nnodes)]
             | q1 : int(1..nrings - 1), q1 + 1 <= nrings]),
    and([and([network_ExplicitWithRepetitionR2_Values_Occurrence[q2, q9] = false | q9 : int(1..nnodes)])
             | q2 : int(1..nrings), q2 > nrings]),
    and([sum([toInt(network_ExplicitWithRepetitionR2_Values_Occurrence[q5, q6]) | q6 : int(1..nnodes)]) <= capacity
             | q5 : int(1..nrings), q5 <= nrings])

