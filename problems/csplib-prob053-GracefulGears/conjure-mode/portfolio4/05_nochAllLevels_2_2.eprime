                    -
                    sum([toInt(colour_FunctionAsRelation_RelationAsMatrix[q77_1, q77_2]) * catchUndef(q77_2, 0)
                             | q77_1 : int(0..let1), q77_2 : int(0..let2), q77_1 = (v + 1) % (2 * n)])|,
                   or([colour_FunctionAsRelation_RelationAsMatrix[q75_1, q75_2] /\ q75_1 = v
                           | q75_1 : int(0..let1), q75_2 : int(0..let2)]),
                   or([colour_FunctionAsRelation_RelationAsMatrix[q80_1, q80_2] /\ q80_1 = (v + 1) % (2 * n)
                           | q80_1 : int(0..let1), q80_2 : int(0..let2)]);
                       int(1..3)]))
                 | q70_1 : int(0..let3), q70_2 : int(0..let4)])
             | v : int(0..2 * n - 1)]),
    and([or([edgeID_FunctionAsRelation_RelationAsMatrix[q17_1, q17_2] /\
             (q17_1 = v / 2 + 2 * n /\
              and([q17_2 =
                   |sum([toInt(colour_FunctionAsRelation_RelationAsMatrix[q27_1, q27_2]) * catchUndef(q27_2, 0)
                             | q27_1 : int(0..let1), q27_2 : int(0..let2), q27_1 = C])
                    -
                    sum([toInt(colour_FunctionAsRelation_RelationAsMatrix[q46_1, q46_2]) * catchUndef(q46_2, 0)
                             | q46_1 : int(0..let1), q46_2 : int(0..let2), q46_1 = v])|,
                   or([colour_FunctionAsRelation_RelationAsMatrix[q39_1, q39_2] /\ q39_1 = C
                           | q39_1 : int(0..let1), q39_2 : int(0..let2)]),
                   or([colour_FunctionAsRelation_RelationAsMatrix[q49_1, q49_2] /\ q49_1 = v
                           | q49_1 : int(0..let1), q49_2 : int(0..let2)]);
                       int(1..3)]))
                 | q17_1 : int(0..let3), q17_2 : int(0..let4)])
             | v : int(0..2 * n - 1), v % 2 = 0]),
    and([1 =
         sum([toInt(edgeID_FunctionAsRelation_RelationAsMatrix[q60_1, q60_2])
                  | q60_1 : int(0..let3), q60_2 : int(0..let4), q60_1 = q3])
             | q3 : int(0..let3)]),
    and([edgeID_FunctionAsRelation_RelationAsMatrix[q86_1, q86_2] /\
         edgeID_FunctionAsRelation_RelationAsMatrix[q87_1, q87_2]
         -> (q86_1 < q87_1 -> q86_2 != q87_2)
             | q86_1 : int(0..let3), q86_2 : int(0..let4), q87_1 : int(0..let3), q87_2 : int(0..let4)]),
    and([1 =
         sum([toInt(colour_FunctionAsRelation_RelationAsMatrix[q61_1, q61_2])
                  | q61_1 : int(0..let1), q61_2 : int(0..let2), q61_1 = q9])
             | q9 : int(0..let1)]),
    and([colour_FunctionAsRelation_RelationAsMatrix[q62_1, q62_2] /\
         colour_FunctionAsRelation_RelationAsMatrix[q63_1, q63_2]
         -> (q62_1 < q63_1 -> q62_2 != q63_2)
             | q62_1 : int(0..let1), q62_2 : int(0..let2), q63_1 : int(0..let1), q63_2 : int(0..let2)])

